package lk.ac.vau.fas.Controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class UserController {
	
	@GetMapping("/")
	public String home() {
		return "home";
	}
	
	@GetMapping("/Category")
	public String Category() {
		return "Category";
	}
}
